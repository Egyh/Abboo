<table class='table'>
  <thead>
    <tr>
      <th>アイコン</th>
      <th>名前</th>
      <th>国籍</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image, class: "rounded-circle border border-dark", size:'30x30' %></td>
        <td><%= user.name %></td>
        <td><%= user.country.name %></td>
        <td><%= link_to 'このユーザーをみる', user, class:"btn btn-info" %></td>
    <% end %>
  </tbody>
</table>
 <div class="paginattion">
        <%= paginate @users %>
  </div>